{
  "name": "eslint-config-mrmlnc",
  "version": "1.1.1",
  "description": "Just my personal shareable ESLint configuration for easy distribution between my packages",
  "license": "MIT",
  "repository": "mrmlnc/eslint-config-mrmlnc",
  "author": {
    "name": "Denis Malinochkin",
    "url": "https://mrmlnc.com"
  },
  "engines": {
    "node": ">=10.13"
  },
  "main": "./out/bundle.js",
  "files": [
    "./out/bundle.js"
  ],
  "keywords": [
    "eslint",
    "config",
    "mrmlnc"
  ],
  "peerDependencies": {
    "eslint": ">=7.23.0",
    "typescript": ">=4.2.3"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.7",
    "@types/mocha": "^8.2.2",
    "@types/node": "^10.17.56",
    "esbuild": "^0.12.9",
    "eslint": "^7.23.0",
    "mocha": "^9.0.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "29.0.0"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "esbuild lib/index.js --bundle --platform=node --outfile=out/bundle.js --external:@typescript-eslint/*",
    "lint": "eslint \"lib/**/*.js\" --cache",
    "test": "mocha \"lib/**/*.spec.js\""
  },
  "eslintConfig": {
    "extends": "./out/bundle.js"
  }
}
