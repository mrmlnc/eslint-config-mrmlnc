{
  "name": "eslint-config-mrmlnc",
  "version": "3.1.0",
  "description": "Just my personal shareable ESLint configuration for easy distribution between my packages",
  "license": "MIT",
  "repository": "mrmlnc/eslint-config-mrmlnc",
  "author": {
    "name": "Denis Malinochkin",
    "url": "https://mrmlnc.com"
  },
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  },
  "main": "./out/bundle.js",
  "files": [
    "out"
  ],
  "keywords": [
    "eslint",
    "config",
    "mrmlnc"
  ],
  "peerDependencies": {
    "eslint": ">=8.39.0",
    "typescript": ">=5.0.4"
  },
  "devDependencies": {
    "@types/eslint": "8.40.2",
    "@types/mocha": "10.0.1",
    "@types/node": "14.18.42",
    "esbuild": "0.18.8",
    "eslint": "8.43.0",
    "mocha": "10.2.0",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "46.0.0"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "esbuild lib/index.js --bundle --platform=node --outfile=out/bundle.js --external:@typescript-eslint/*",
    "lint": "eslint \"lib/**/*.js\" --cache",
    "test": "mocha \"lib/**/*.spec.js\""
  },
  "eslintConfig": {
    "extends": "./out/bundle.js"
  }
}
