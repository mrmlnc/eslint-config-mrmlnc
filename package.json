{
  "name": "eslint-config-mrmlnc",
  "version": "4.0.1",
  "description": "Just my personal shareable ESLint configuration for easy distribution between my packages",
  "license": "MIT",
  "repository": "mrmlnc/eslint-config-mrmlnc",
  "author": {
    "name": "Denis Malinochkin",
    "url": "https://mrmlnc.ru"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./out/bundle.js",
  "files": [
    "./out/bundle.js"
  ],
  "keywords": [
    "eslint",
    "config",
    "mrmlnc"
  ],
  "peerDependencies": {
    "eslint": ">=8.51.0",
    "typescript": ">=5.1.6"
  },
  "devDependencies": {
    "@types/eslint": "8.44.3",
    "@types/node": "16.18.50",
    "esbuild": "0.19.4",
    "eslint": "8.51.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.4",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-mocha": "10.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "48.0.1"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "esbuild lib/index.js --bundle --platform=node --outfile=out/bundle.js --external:@typescript-eslint/*",
    "lint": "eslint \"lib/**/*.js\" --cache",
    "test": "node --test ./lib"
  },
  "eslintConfig": {
    "extends": "./out/bundle.js"
  }
}
